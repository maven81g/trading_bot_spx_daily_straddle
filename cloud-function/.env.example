# Environment Variables for SPX Daily Trader Cloud Function
# Copy this to .env for local development

# Google Cloud Configuration
GOOGLE_CLOUD_PROJECT=your-project-id-12345
BIGQUERY_DATASET=spx_trading

# TradeStation API (all lowercase)
ts_client_id=your_tradestation_client_id
ts_client_secret=your_tradestation_client_secret
ts_refresh_token=your_tradestation_refresh_token

# Mailgun Email Configuration (all lowercase)
mailgun_api_key=your_mailgun_api_key
# Note: mailgun_domain, email_to, email_from are hardcoded in the application

# Optional Configuration
CLOUD_FUNCTION_VERSION=1.0.0
NODE_ENV=production