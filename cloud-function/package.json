{
  "name": "spx-daily-trader",
  "version": "1.0.0",
  "description": "Daily SPX options trading with MACD strategy - Google Cloud Function",
  "main": "main.js",
  "scripts": {
    "start": "functions-framework --target=main",
    "test": "echo \"Error: no test specified\" && exit 1",
    "setup-bigquery": "python setup_bigquery.py",
    "build": "node build-dist.js",
    "deploy": "gcloud functions deploy spx-daily-trader --runtime nodejs18 --trigger-http --entry-point main --memory 512MB --timeout 540s"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@google-cloud/functions-framework": "^3.3.0",
    "mailgun-js": "^0.22.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "trading",
    "options",
    "spx",
    "macd",
    "google-cloud",
    "bigquery",
    "mailgun"
  ],
  "author": "SPX Trading Bot",
  "license": "MIT"
}