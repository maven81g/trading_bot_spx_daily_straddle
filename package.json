{
  "name": "spx-straddle-bot",
  "version": "1.0.0",
  "description": "SPX Daily Straddle Trading Bot for TradeStation API",
  "main": "dist/index-straddle.js",
  "types": "dist/index-straddle.d.ts",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r dotenv/config src/index-straddle.ts",
    "start:prod": "node dist/index-straddle.js",
    "start:legacy": "ts-node -r dotenv/config src/index.ts",
    "backtest": "node spx-straddle-backtest.js",
    "test-spx": "ts-node -r dotenv/config test-spx-backtest.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "watch": "tsc --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "trading",
    "bot",
    "tradestation",
    "algorithmic-trading",
    "typescript",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/bigquery": "^7.3.0",
    "@types/express": "^5.0.3",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "ws": "^8.14.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.195",
    "@types/node": "^20.5.0",
    "@types/uuid": "^9.0.2",
    "@types/winston": "^2.4.4",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
